{% extends 'base.html.twig' %}

{% block title %}Symptom{% endblock %}

{% block body %}
    <h1>Symptom</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ symptom.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ symptom.type }}</td>
            </tr>
            <tr>
                <th>Intensite</th>
                <td>{{ symptom.intensite }}</td>
            </tr>
            <tr>
                <th>Date_observation</th>
                <td>{{ symptom.dateObservation ? symptom.dateObservation|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_symptom_index') }}">back to list</a>

    <a href="{{ path('app_symptom_edit', {'id': symptom.id}) }}">edit</a>

    {{ include('symptom/_delete_form.html.twig') }}
{% endblock %}
